{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": ["/images/*.{png,jpg,gif,ico}", "/css/*", "/api/*"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:;"
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "platform": {
    "apiRuntime": "dotnet-isolated:8.0"
  }
}
